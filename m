diff --git a/mm/util.c b/mm/util.c
index ad8f8c482d14..9e0c86555adf 100644
--- a/mm/util.c
+++ b/mm/util.c
@@ -1073,3 +1073,36 @@ int __weak memcmp_pages(struct page *page1, struct page *page2)
